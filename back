var firstnumber;
var secondnumber;
document.getElementById("mybutton").onclick = function(){
    firstnumber = document.getElementById("firsttext").value;
    secondnumber = document.getElementById("secondtext").value;
    console.log("Even numbers")
    for( firstnumber = 1; firstnumber <= secondnumber; firstnumber++){
        if(firstnumber %2 == 0){
            console.log(firstnumber);
        }
    }
}
